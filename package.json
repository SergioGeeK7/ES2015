{
  "name": "peliculas",
  "descripcion": "web opa aprendiendo juntos",
  "version": "0.1.0",
  "dependencies": {
    "jquery": "^2.2.3"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babelify": "^7.3.0",
    "browserify": "^13.0.0"
  },
  "scripts": {
    "public":"mkdir public",
    "build-js": "browserify -t [ babelify --presets [ es2015 ] ] index.js > public/app.js",
    "copy":"copy index.css public/app.css && copy index.html public/app.html",
    "build":"npm run public && npm run build-js && npm run copy"
  }
}
